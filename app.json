{
  "expo": {
    "name": "reMIND",
    "slug": "reMIND",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/cookieSplash.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/images/cookieSplash.png",
      "resizeMode": "cover",
      "backgroundColor": "#fff"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.ceunnseo.reMIND",
      "usesAppleSignIn": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/cookieSplash.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ceunnseo.reMIND",
      "permissions": ["INTERNET"],
      "googleServicesFile": "./google-services.json"
    },
    "scheme": "com.ceunnseo.reMIND",
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "1cd0480c-0399-4503-ae2d-ec73641ea4fd"
      }
    },
    "plugins": [
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.94079762653-arcgeib4l0hbg6snh81cjimd9iuuoun3"
        }
      ],
      [
        "expo-font",
        {
          "Pretend-Black": ["./assets/fonts/Pretendard-Black.ttf"],
          "Pretend-Bold": ["./assets/fonts/Pretendard-Bold.ttf"],
          "Pretend-ExtraBold": ["./assets/fonts/Pretendard-ExtraBold.ttf"],
          "Pretend-ExtraLight": ["./assets/fonts/Pretendard-ExtraLight.ttf"],
          "Pretend-Light": ["./assets/fonts/Pretendard-Light.ttf"],
          "Pretendard-Medium": ["./assets/fonts/Pretendard-Medium.ttf"],
          "Pretend-Regular": ["./assets/fonts/Pretendard-Regular.ttf"],
          "Pretend-SemiBold": ["./assets/fonts/Pretendard-SemiBold.ttf"],
          "Pretend-Thin": ["./assets/fonts/Pretendard-Thin.ttf"]
        }
      ],
      [
        "expo-notifications",
        {
          "color": "#ffffff",
          "mode": "production"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ],
      "expo-apple-authentication",
      "expo-localization",
      "expo-build-properties"
    ],
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/1cd0480c-0399-4503-ae2d-ec73641ea4fd"
    }
  }
}
